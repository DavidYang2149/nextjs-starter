{
  "name": "@davidyang2149/nextjs-starter",
  "version": "2.2.2",
  "description": "Next.js starter with TypeScript, Jest, Testing-Library, Cypress",
  "bin": {
    "@davidyang2149/nextjs-starter": "./bin/create-nextjs-starter.js"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watchAll --coverage --detectOpenHandles",
    "test:unit": "jest",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 cypress:headless",
    "report:merge": "mochawesome-merge cypress/results/json/*.json > index.json",
    "report:generate": "marge index.json --reportDir public --assetsDir public/assets --reportPageTitle index.html"
  },
  "dependencies": {
    "commander": "^9.4.0",
    "cypress-multi-reporters": "^1.6.1",
    "next": "12.3.1",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^13.3.0",
    "@types/jest-plugin-context": "^2.9.4",
    "@types/mochawesome": "^6.2.1",
    "@types/node": "16.11.9",
    "@types/react": "18.0.15",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-jest": "^27.3.1",
    "cypress": "^10.4.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-next": "12.0.4",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jest-plugin-context": "^2.9.0",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "start-server-and-test": "^1.14.0",
    "supertest": "^6.1.6",
    "typescript": "^4.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidYang2149/nextjs-starter.git"
  },
  "keywords": [
    "Next.js",
    "nextjs",
    "Next.js starter",
    "nextjs starter",
    "starter",
    "typescript",
    "eslint",
    "jest",
    "testing-library",
    "cypress"
  ],
  "author": "davidyang2149",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DavidYang2149/nextjs-starter/issues"
  },
  "homepage": "https://github.com/DavidYang2149/nextjs-starter#readme"
}
